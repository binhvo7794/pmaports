# Maintainer: Sicelo <absicsz@gmail.com>
pkgname=gps-nokia-n900
pkgver=0.3.2
pkgrel=1
pkgdesc="GPS support for Nokia N900"
url="https://git.maemo.org/leste/gps-nokia-n900"
arch="armv7"
license="GPL-3.0-only"
makedepends="linux-headers"
source="$pkgname-$pkgver.tar.gz::https://git.maemo.org/leste/gps-nokia-n900/archive/$pkgver.tar.gz"
options="!check"

build() {
	make
}

package() {
	install -D -m755 "$builddir"/gps-nokia-n900 -t "$pkgdir"/usr/bin/
}

