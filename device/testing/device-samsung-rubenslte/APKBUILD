pkgname=device-samsung-rubenslte
pkgdesc="Samsung Galaxy Tab Active LTE
pkgver=1
pkgrel=0
url="https://postmarketos.org"
license="MIT"
arch="armv7"
options="!check !archcheck"
depends="postmarketos-base linux-samsung-rubenslte mkbootimg msm-fb-refresher"
makedepends="devicepkg-dev"
source="deviceinfo"

build() {
	devicepkg_build $startdir $pkgname
}

package() {
	devicepkg_package $startdir $pkgname
}


sha512sums="(run 'pmbootstrap checksum device-samsung-rubenslte' to fill)"
